&hsi2c_6 {
	status = "okay";
	samsung,reset-before-trans;
	clock-frequency = <400000>;

	aw8695_haptic@5A {
		compatible = "awinic,aw8695_haptic";
		reg = <0x5A>;
		pinctrl-names = "lra_int_status";
		pinctrl-0 = <&lra_int_status>;
		reset-gpio = <&gpa2 5 0>;
		irq-gpio = <&gpa2 7 0>;
		aw8695,cont_drv_lvl = <98>;
		aw8695,cont_drv_lvl_ov = <98>;
	};
};

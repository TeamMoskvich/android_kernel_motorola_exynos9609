/* I2C_CAM0 */ /* SENSOR_CONTROL_I2C0 */
&hsi2c_12 {
	gpios = <&gpc0 0 0 &gpc0 1 0>;
	status = "okay";
	clock-frequency = <900000>;
	samsung,reset-before-trans;

	samsung,fast-plus-mode;
	#address-cells = <1>;
	#size-cells = <0>;

	fimc-is-12a10@36 {
		compatible = "samsung,exynos5-fimc-is-cis-12a10";
		reg = <0x36>;
		id = <0>; /* matching fimc_is_sensor id */
		setfile = "setA";
		use_initial_ae; /* for initial AE */
		dual_sync_mode = <DUAL_SYNC_SLAVE>;
	};

	fimc-is-actuator@72 {
		compatible = "samsung,exynos5-fimc-is-actuator-lc898217";
		reg = <0x72>;
		id = <0>; /* matching fimc_is_sensor id */
		place = <0>;
	};

        fimc-is-eeprom@50 {
                compatible = "samsung,exynos5-fimc-is-sensor-eeprom-12a10";
                reg = <0x50>;
                id = <0>;
        };
};

